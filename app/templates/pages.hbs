<div class="center-wrapper content-wrapper">
  <h1>
    {{t 'pages.title'}}
  </h1>

  {{#quick-links}}
    {{link-to (t 'links.create_page') 'page-create'}}
  {{/quick-links}}

  <ol>
    {{#each model as |page|}}
      <li>
        {{link-to page.title 'page' page.alias}}
      </li>
    {{else}}
      {{t 'messages.empty'}}
    {{/each}}
  </ol>
</div>
